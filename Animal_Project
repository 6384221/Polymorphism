class Animal:
    def __init__(self, name):
        self.name = name
    
    def make_sound(self):
        pass

class Dog(Animal):
    def make_sound(self):
        print(self.name + " barks")

class Cat(Animal):
    def make_sound(self):
        print(self.name + " meows")

dog = Dog("Buddy")
cat = Cat("Fluffy")

dog.make_sound() # Output: Buddy barks
cat.make_sound() # Output: Fluffy meows
